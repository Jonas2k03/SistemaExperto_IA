// Agrimony – tortura mental detrás de una cara alegre
rule "Recetar Agrimony"
when
    $paciente : Paciente(sintomas contains "tortura mental" || sintomas contains "cara alegre")
then
    EscenciaFloral esencia = new EscenciaFloral("Agrimony", "agrimony.jpg");
    $paciente.addRecomendacion(esencia);
    System.out.println("Recomendación: Recetar " + esencia.getNombre() + " para el paciente " + $paciente.getNombre());
end

